{
	"name": "js-orm",
	"version": "0.0.1",
	"description": "ORM",
	"main": "./lib/js-orm",
	"bin": {
		"config-extends": "bin/js-orm"
	},
	"scripts": {
		"start": "node dist/app/index.js",
		"predev": "npm run swagger",
		"prebuild": "npm run swagger",
		"build-all": "node config/build.js && tsc",
		"build": "tsc",
		"dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
		"swagger": "tsoa spec",
		"standard": "standard",
		"test": "mocha",
		"test-populate-source": "node dist/test/populateSource.js"
	},
	"nodemonConfig": {
		"watch": [
			"src"
		],
		"ext": "ts",
		"exec": "ts-node src/app/index.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FlavioLionelRita/js-orm.git"
	},
	"keywords": [
		"orm",
		"sql",
		"query",
		"builder",
		"expression",
		"parser",
		"database",
		"data access"
	],
	"author": "Flavio Lionel Rita <flaviolrita@hotmail.com>",
	"license": "Apache-2.0 License",
	"bugs": {
		"url": "https://github.com/FlavioLionelRita/js-orm/issues"
	},
	"homepage": "https://github.com/FlavioLionelRita/js-orm#readme",
	"dependencies": {
		"express": "^4.17.1",
		"generic-pool": "^3.7.8",
		"luxon": "^2.0.2",
		"morgan": "^1.10.0",
		"mysql2": "^2.2.5",
		"pg": "^8.7.1",
		"reflect-metadata": "^0.1.13",
		"sqlstring": "^2.3.2",
		"swagger-ui-express": "^4.1.6",
		"tsoa": "^3.8.0"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.15.4",
		"@types/express": "^4.17.12",
		"@types/morgan": "^1.9.2",
		"@types/mysql": "^2.15.18",
		"@types/node": "^15.12.2",
		"@types/swagger-ui-express": "^4.1.2",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"chai": "^4.2.0",
		"concurrently": "^6.2.0",
		"config-extends": "^1.0.4",
		"dotenv": "^10.0.0",
		"eslint": "^7.32.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.24.2",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^5.1.0",
		"grunt": "^1.4.1",
		"grunt-contrib-concat": "^1.0.1",
		"grunt-contrib-jshint": "^3.0.0",
		"grunt-contrib-uglify": "^5.0.1",
		"grunt-mysql-query": "0.0.1",
		"mocha": "^8.2.1",
		"nodemon": "^2.0.7",
		"ts-node": "^10.0.0",
		"typescript": "^4.3.5"
	}
}