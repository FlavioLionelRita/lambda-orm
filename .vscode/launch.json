{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "lab",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/dist/lab/test.js",
			"envFile": "${workspaceFolder}/src/test/test.env"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "app",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/dist/app/index.js",
			"envFile": "${workspaceFolder}/src/test/test.env"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "generate model",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/dist/test/generateModel.js",
			"envFile": "${workspaceFolder}/src/test/test.env"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "generate data test files",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/dist/test/generateDataForTest.js",
			"envFile": "${workspaceFolder}/src/test/test.env"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "test complete expressions",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/dist/test/completeExpressions.js",
			"envFile": "${workspaceFolder}/src/test/test.env"
		},
		{
			"command": "npm run test",
			"name": "test",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"name": "populate source",
			"type": "node",
			"request": "launch",
			"program": "/usr/bin/grunt",
			"args": [
				"populate_source"
			],
			"stopOnEntry": false
		},
		{
			"name": "populate databases",
			"type": "node",
			"request": "launch",
			"program": "/usr/bin/grunt",
			"args": [
				"populate_databases"
			],
			"stopOnEntry": false
		},
		{
			"name": "generate data for test",
			"type": "node",
			"request": "launch",
			"program": "/usr/bin/grunt",
			"args": [
				"generate_data_for_test"
			],
			"stopOnEntry": false
		},
		{
			"type": "node",
			"request": "launch",
			"name": "update config",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/config/build.js"
		},
		{
			"command": "npm run swagger",
			"name": "Run npm swagger",
			"request": "launch",
			"type": "node-terminal"
		},
	]
}