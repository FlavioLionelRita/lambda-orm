	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "lab",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/test/lab/lab.ts",
			"envFile": "${workspaceFolder}/src/test/test.env"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "generate model",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/test/task/generateModel.ts",
			"envFile": "${workspaceFolder}/src/test/test.env"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "generate data test files",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/test/generateDataForTest.ts",
			"envFile": "${workspaceFolder}/src/test/test.env"
		},
		{
			"name": "drop dbs",
			"type": "node",
			"request": "launch",
			"program": "/usr/bin/grunt",
			"args": [
				"drop_dbs"
			],
			"stopOnEntry": false
		},
		{
			"name": "clean data",
			"type": "node",
			"request": "launch",
			"program": "/usr/bin/grunt",
			"args": [
				"clean_data"
			],
			"stopOnEntry": false
		},
		{
			"name": "populate source",
			"type": "node",
			"request": "launch",
			"program": "/usr/bin/grunt",
			"args": [
				"populate_source"
			],
			"stopOnEntry": false
		},
		{
			"name": "populate databases",
			"type": "node",
			"request": "launch",
			"program": "/usr/bin/grunt",
			"args": [
				"populate_databases"
			],
			"stopOnEntry": false
		},
